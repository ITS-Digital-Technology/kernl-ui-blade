{
    "name": "northeastern-web/kernl-ui-blade",
    "description": "Package of Blade components for Northeastern University websites",
    "keywords": [
        "northeastern-web",
        "support"
    ],
    "homepage": "https://github.com/northeastern-web/kernl-ui-blade",
    "license": "MIT",
    "authors": [
        {
            "name": "Northeastern Web Solutions",
            "email": "websolutions@northeastern.edu",
            "homepage": "https://northeastern.edu",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2",
        "illuminate/support": "^7.0|^8.0",
        "illuminate/view": "^7.0|^8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16"
    },
    "autoload": {
        "psr-4": {
            "Northeastern\\Blade\\": "src"
        }
    },
    "scripts": {
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Northeastern\\Blade\\NUKernlUIBladeComponentsServiceProvider"
            ]
        }
    }
}
